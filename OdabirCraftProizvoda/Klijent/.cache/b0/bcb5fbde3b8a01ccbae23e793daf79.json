oduct/ProductDetails.js","original":{"line":75,"column":15}}],"sources":{"product/ProductDetails.js":"import { Link, useParams } from \"@reach/router\";\r\nimport React, { useEffect, useState } from \"react\";\r\n\r\n\r\nconst ProductDetails = (props) => {\r\n    const [product, setProduct] = useState([]);\r\n\r\n    useEffect(() => {\r\n        fetch(\"http://localhost:5000/api/products/\" + props.productName)\r\n        .then((response) => response.json())\r\n        .then((product) => { \r\n            setProduct(product)});\r\n    }, []);\r\n\r\n    function deleteProduct(productName) {\r\n        const options = {headers:{\r\n            Authorization: \"Bearer \" + localStorage.getItem(\"token\")\r\n        }};\r\n        const config = {\r\n            method: \"DELETE\",\r\n            headers: {\r\n            \"Content-Type\": \"application/json\" \r\n            },\r\n            body: JSON.stringify({productName})\r\n        }\r\n        fetch(\"http://localhost:5000/api/product/\" + productName, options, config)\r\n        .then(response => response.json())\r\n        .then(response => {\r\n            if (response.error) {\r\n                alert(response.error);\r\n              } else {\r\n                alert(`${response} DELETED!`);\r\n                navigate(\"/\");\r\n              }  \r\n        });\r\n    }\r\n\r\n    return(\r\n        <div>\r\n            <table>\r\n                <thead>\r\n                    <tr>\r\n                        <th>Product name</th>\r\n                        <th>Color</th>\r\n                        <th>Type</th>\r\n                        <th>Company name</th>\r\n                        <th>Price</th>\r\n                        <th>Alcohol Percentage</th>\r\n                        <th>Actions</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    {product && product.map(p => \r\n                        <tr key={p.productName}>\r\n                            <td className=\"name\">{p.productName}</td>\r\n                            <td>{p.color}</td>\r\n                            <td>{p.type}</td>\r\n                            <td>{p.companyName}</td>\r\n                            <td>${p.price}</td>\r\n                            <td>{p.alcoholPercentage}</td>\r\n                            <td>\r\n                                <button onClick={() => deleteProduct(p.productName)}>Delete</button>\r\n                                <Link to={\"/product/update/\" + p.productName}>\r\n                                    <button>Update</button>\r\n                                </Link>\r\n                            </td>\r\n                        </tr>\r\n                    )}\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default ProductDetails;"},"lineCount":null}},"error":null,"hash":"d9b96d104cacc79db08f28a45f06c8e9","cacheData":{"env":{}}}