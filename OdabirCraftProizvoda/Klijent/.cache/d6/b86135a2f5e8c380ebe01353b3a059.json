(companyName) {\r\n        console.log(\"RAAADIIIII\", companyName);\r\n    }\r\n\r\n    return(\r\n        <div className=\"search-params\">\r\n            {/* check how to hide buttons */}\r\n            <button onClick={() => navigate('/login')}>Login</button>\r\n            <button onClick={() => navigate('/register')}>Register</button>\r\n            <button onClick={() => navigate('/logout')}>Logout</button>\r\n            \r\n            <table>\r\n                <thead>\r\n                    <tr>\r\n                        <th>Company name</th>\r\n                        <th>Country</th>\r\n                        <th>Establishment year</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    {companies && companies.map(comp => \r\n                        <tr key={comp.name}>\r\n                            <td onClick={() => navigateToCompanyDetail(comp.name)} className=\"name\">{comp.name}</td>\r\n                            <td>{comp.country}</td>\r\n                            <td>{comp.establishmentYear}</td>\r\n                        </tr>\r\n                    )}\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default SearchParams;"},"lineCount":null}},"error":null,"hash":"b61ede0e515b3b2c7ca549c5eb63b65e","cacheData":{"env":{}}}