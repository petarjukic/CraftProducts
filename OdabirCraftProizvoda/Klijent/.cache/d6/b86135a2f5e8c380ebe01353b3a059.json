e":25,"column":4}},{"generated":{"line":57,"column":0},"source":"SearchParams.js","original":{"line":61,"column":1}},{"generated":{"line":57,"column":1},"source":"SearchParams.js","original":{"line":6,"column":0}},{"name":"SearchParams","generated":{"line":59,"column":15},"source":"SearchParams.js","original":{"line":63,"column":15}}],"sources":{"SearchParams.js":"import React, {useState, useEffect, useContext} from \"react\";\r\nimport { Link, navigate } from \"@reach/router\";\r\nimport { UserContext } from \"./UserContext\";\r\n\r\n\r\nconst SearchParams = () => {\r\n    const [companies, setCompanies] = useState([]);\r\n    const {user, setUser} = useContext(UserContext);\r\n\r\n\r\n    useEffect(() => {\r\n        const options = {headers:{\r\n            Authorization: \"Bearer \" + localStorage.getItem(\"token\")\r\n        }};\r\n\r\n        fetch(\"http://localhost:5000/api/company\", options)\r\n        .then((response) => response.json())\r\n        .then((companies) => {\r\n            console.log(\"OVO JE EMAIL \", user);\r\n            companies = companies.sort((a, b) =>(a.name > b.name) ? 1 : -1);\r\n            setCompanies(companies)\r\n        });\r\n    },[]);\r\n\r\n    return(\r\n        <div className=\"search-params\">\r\n            {user ? \r\n                <button onClick={() => navigate('/logout')}>Logout</button> : \r\n                <div> \r\n                    <button onClick={() => navigate('/login')}>Login</button>\r\n                    <button onClick={() => navigate('/register')}>Register</button>\r\n                </div>\r\n            }\r\n\r\n            <button onClick={() => navigate('/products')}>Products</button>\r\n            <button onClick={() => navigate('/createProduct')}>Insert Product</button>\r\n            <button onClick={() => navigate('/createCompany')}>Insert Company</button>\r\n            \r\n            <table>\r\n                <thead>\r\n                    <tr>\r\n                        <th>Company name</th>\r\n                        <th>Country</th>\r\n                        <th>Establishment year</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    {companies && companies.map(comp => \r\n                        <tr key={comp.name}>\r\n                            <Link to={\"/company/details/\" + comp.name}>\r\n                                <td className=\"name\">{comp.name}</td>\r\n                            </Link>\r\n                            <td>{comp.country}</td>\r\n                            <td>{comp.establishmentYear}</td>\r\n                        </tr>\r\n                    )}\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default SearchParams;"},"lineCount":null}},"error":null,"hash":"b0c66b9c3c35607eb7b45f1244fabd0c","cacheData":{"env":{}}}