       <th>Alcohol Percentage</th>\r\n                        <th>Actions</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    {products && products.map(p => \r\n                        <tr key={p.productName}>\r\n                            <td className=\"name\">{p.productName}</td>\r\n                            <td>{p.color}</td>\r\n                            <td>{p.type}</td>\r\n                            <td>{p.companyName}</td>\r\n                            <td>${p.price}</td>\r\n                            <td>{p.alcoholPercentage}</td>\r\n                            <td>\r\n                                {isAdmin ? \r\n                                    <div>\r\n                                        <button onClick={() => deleteProduct(p.productName)}>Delete</button>\r\n                                        <Link to={\"/product/update/\" + p.productName}>\r\n                                            <button>Update</button>\r\n                                        </Link>\r\n                                    </div>\r\n                                    :\r\n                                    <div>\r\n                                    </div>\r\n                                }\r\n                            </td>\r\n                        </tr>\r\n                    )}\r\n                </tbody>\r\n            </table>\r\n            <button onClick={() => navigate('/')}>Home</button>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default CompanyProducts;"},"lineCount":null}},"error":null,"hash":"cd7d1ef33e0e5186136fd25659ee7f69","cacheData":{"env":{}}}